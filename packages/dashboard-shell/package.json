{
  "name": "dashboard-shell",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 3000",
    "dev:fast": "vite --port 3000 --force --no-optimize-deps",
    "build": "tsc && vite build",
    "build:dev": "vite build --mode development",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "preview": "vite preview --port 3000",
    "clean": "rimraf dist",
    "clean:cache": "rimraf node_modules/.vite",
    "optimize": "node scripts/dev-optimize.js",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:integration": "vitest --config vitest.integration.config.ts",
    "test:integration:run": "vitest run --config vitest.integration.config.ts",
    "test:integration:coverage": "vitest run --coverage --config vitest.integration.config.ts",
    "test:integration:runner": "node scripts/run-integration-tests.js",
    "test:integration:suite": "node scripts/run-integration-tests.js suite",
    "test:integration:analyze": "node scripts/run-integration-tests.js analyze",
    "test:all": "npm run test:run && npm run test:integration:run",
    "test:watch": "vitest --watch",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@niobi/shared-components": "*",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.1",
    "reconciliation-tool": "*"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.13",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/ui": "^1.6.0",
    "autoprefixer": "^10.4.21",
    "jsdom": "^24.1.0",
    "rimraf": "^5.0.5",
    "rollup-plugin-visualizer": "^6.0.3",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.5.3",
    "vite": "^5.4.2",
    "vitest": "^1.6.0"
  },
  "description": "",
  "main": "index.js",
  "keywords": [],
  "author": "",
  "license": "ISC"
}
